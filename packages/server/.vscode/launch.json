{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Boilerplate Server",
      "program": "${workspaceFolder}\\src\\main.ts",
      "env": {
        "NODE_ENV": "DEV"
      },
      "smartStep": true,
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "sourceMaps": true
    }
  ]
}
